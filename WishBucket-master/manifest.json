{
  "manifest_version": 2,

  "name": "Getting started example",
  "description": "This extension adds items from a website into wishlists",
  "version": "2.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [{
     "js": [ "js/jquery-2.2.0.min.js", "js/materialize.js", "js/render.js" ],
     "matches": ["http://*/*", "https://*/*"]
  }],
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
