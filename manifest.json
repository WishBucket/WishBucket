{
  "manifest_version": 3,

  "name": "WishBucket Extension",
  "description": "This extension allows a user to add items to a universal wishlist",
  "version": "3.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [{
     "js": [ "js/imagescript.js" ],
     "matches": ["http://*/*", "https://*/*"]
  }],
  "icons": {
  		"48": "icon48.png",
  		"128": "icon128.png"},
  
  "permissions": [
	"storage",
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
